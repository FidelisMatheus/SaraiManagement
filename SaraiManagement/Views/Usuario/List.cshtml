@model SaraiManagement.Models.ViewModels.UsuarioListViewModel

@{
    ViewData["Title"] = "List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Lista de Usuarios</h1>

@foreach (var d in Model.Usuarios)
{
    <div class="card card-outline-primary m-1 p-1">
        <div class="bg-faded p-1">
            <h4>
                <a asp-action="Consultar" asp-route-id="@d.UsuarioID">@d.Nome </a>
                <span class="badge badge-pill badge-primary" style="float:right">
                    <small>@d.Tipo</small>
                </span>
           </h4>
        </div>
    </div>
}

<div page-model="@Model.PagingInfo" paging-action="List" page-classes-enabled="true" page-class="btn" page-class-normal="btn-secondary"
     page-class-selected="btn-primary" class="btn-group pull-right m-1">

</div>